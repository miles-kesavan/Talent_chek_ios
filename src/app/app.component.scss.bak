ion-menu {
    --ion-text-color: var(--ion-color-primary-contrast);
    ion-list {
        background: transparent;
        //padding-left: 15px;
    }
}

ion-menu.md ion-content {
    --padding-start: 8px;
    --padding-end: 8px;
    --padding-top: 20px;
    --padding-bottom: 20px;
}

ion-menu.ios ion-content {
    --padding-start: 8px;
    --padding-end: 8px;
    --padding-top: 20px;
    --padding-bottom: 20px;
}

ion-menu.md ion-note {
    margin-bottom: 30px;
}

ion-menu.md ion-list-header,
ion-menu.md ion-note {
    padding-left: 10px;
}

ion-menu.md ion-list.inbox-list ion-list-header {
    font-size: 22px;
    font-weight: 600;
    min-height: 20px;
}

ion-menu.md ion-list#labels-list ion-list-header {
    font-size: 16px;
    margin-bottom: 18px;
    color: #757575;
    min-height: 26px;
}

ion-menu.md ion-item {
    --padding-start: 10px;
    --padding-end: 10px;
    border-radius: 4px;
}

ion-menu.ios ion-item {
    --padding-start: 10px;
    --padding-end: 10px;
    border-radius: 4px;
}

ion-menu.md ion-item ion-icon {
    color: #203864;
    margin-right: 15px;
}

ion-menu.ios ion-item ion-icon {
    color: #203864;
    margin-right: 15px;
}

ion-menu.md ion-item ion-label {
    font-weight: 500;
    /*   color:#E33A33; */
    font-size: 15px;
    color: #203864;
}

ion-menu.ios ion-item ion-label {
    font-weight: 500;
    /*   color:#E33A33; */
    font-size: 15px;
    color: #203864;
}

ion-menu.ios ion-content {
    --padding-bottom: 20px;
}

ion-menu.ios ion-list {
    padding: 20px 0 0 0;
}

ion-menu.ios ion-note {
    line-height: 24px;
    margin-bottom: 20px;
}

ion-menu.ios ion-item {
    --padding-start: 16px;
    --padding-end: 16px;
    --min-height: 50px;
}

ion-menu.ios ion-item.selected ion-icon {
    color: var(--ion-color-primary);
}

ion-menu.ios ion-item ion-icon {
    font-size: 24px;
    color: #203864;
}

ion-menu.ios ion-list#labels-list ion-list-header {
    margin-bottom: 8px;
}

ion-menu.ios ion-list-header,
ion-menu.ios ion-note {
    padding-left: 16px;
    padding-right: 16px;
}

ion-menu.ios ion-note {
    margin-bottom: 8px;
}

ion-note {
    display: inline-block;
    font-size: 16px;
    color: var(--ion-color-medium-shade);
}

ion-item.selected {
    --color: var(--ion-color-primary);
}

ion-menu.md ion-item ion-label {
    font-family: 'Poppins', sans-serif;
}

.menu-list {
    margin: 30px 0px;
}

@media (max-width: 360px) {
    ion-menu.md ion-item ion-label {
        font-size: 14px;
    }
    .menu-icon.md {
        font-size: 19px;
    }
}

@media (min-width: 361px) and (max-width: 480px) {
    ion-menu.ios ion-item ion-label {
        font-size: 13px;
    }
    .menu-icon.ios {
        font-size: 17px !important;
    }
}

ion-content {
    // --background: url('/assets/img/BG2.jpg') 0 0/100% 100% no-repeat;
    --background: linear-gradient(to right top, #f5f5f5, #f8ecbe);
}

// ion-item {
//   --background: url('/assets/img/BG3.jpg') 0 0/100% 100% no-repeat;
// }
.ParentMenu {
    // --background: url('/assets/img/BG3.jpg') 0 0/100% 100% no-repeat;
    --background: linear-gradient(to right top, #f8d239, orange);
}

.ChildMenu {
    // --background: url('/assets/img/BG10.jpg') 0 0/100% 100% no-repeat;
    --background: linear-gradient(to right top, #f8dd70, #fac906);
}

.menuHeaderStyle {
    padding-top: 20px;
    padding-bottom: 20px;
    // background-color: yellow;
    background: linear-gradient(to right top, #fceeb5, #fad232);
}
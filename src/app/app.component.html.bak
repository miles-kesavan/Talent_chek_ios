				<!-- Individual menu -->
				<div *ngIf="IsIndividual == true">
					<ion-menu-toggle auto-hide="false">
						<ion-item lines="none" (click)="goto_Dashboard()" class="ParentMenu">
							<ion-icon slot="start" name="home"></ion-icon>
							<ion-label> {{dashboardLabel}} </ion-label>
						</ion-item>
					</ion-menu-toggle>
					<div *ngFor="let p of AfterLogin">
						<ion-menu-toggle *ngIf="p.url">
							<ion-item [routerLink]="[p.url]" routerDirection="root" routerLinkActive="active"
								lines="none" detail="false" class="ParentMenu">
								<ion-icon name="{{p.icon}}" slot="start" class="menu-icon"></ion-icon>
								<ion-label class="ion-text-wrap">{{ p.title }}</ion-label>
							</ion-item>
						</ion-menu-toggle>

						<ion-item button *ngIf="p.children?.length > 0" (click)="p.open = !p.open"
							[class.active-parent]="p.open" detail="false" class="ParentMenu">
							<ion-icon name="{{p.icon}}" slot="start" class="menu-icon"></ion-icon>
							<ion-label class="ion-text-wrap">{{ p.title }}</ion-label>
							<ion-icon slot="end" name="caret-down-circle-outline" *ngIf="!p.open"></ion-icon>
							<ion-icon slot="end" name="caret-up-circle-outline" *ngIf="p.open"></ion-icon>
						</ion-item>

						<ion-list *ngIf="p.open">
							<ion-menu-toggle>
								<div *ngFor="let sub of p.children">
									<ion-item
										*ngIf="(!IsStudentOnly || (IsStudentOnly && sub.url != '/professional-experience'))"
										[routerLink]="[sub.url]" routerDirection="root" routerLinkActive="active"
										lines="none" detail="false" class="ChildMenu">
										<ion-icon name="{{sub.icon}}" slot="start" class="menu-icon"></ion-icon>
										<ion-label class="ion-text-wrap">{{ sub.title }}</ion-label>
									</ion-item>
								</div>
							</ion-menu-toggle>
						</ion-list>
					</div>
					<ion-menu-toggle auto-hide="false">
						<ion-item lines="none" (click)="logOut()" class="ParentMenu">
							<ion-icon slot="start" name="log-out-outline"></ion-icon>
							<ion-label> {{logoutLabel}} </ion-label>
						</ion-item>
					</ion-menu-toggle>
				</div>

				<!-- Corporate menu -->
				<div *ngIf="IsIndividual == false && IsInstitution == false">
					<ion-menu-toggle auto-hide="false">
						<ion-item lines="none" (click)="goto_Dashboard()" class="ParentMenu">
							<ion-icon slot="start" name="home"></ion-icon>
							<ion-label> {{dashboardLabel}} </ion-label>
						</ion-item>
					</ion-menu-toggle>
					<div *ngFor="let p of AfterLoginCorporate">
						<ion-menu-toggle *ngIf="p.url">
							<ion-item [routerLink]="[p.url]" routerDirection="root" routerLinkActive="active"
								lines="none" detail="false" class="ParentMenu">
								<ion-icon name="{{p.icon}}" slot="start" class="menu-icon"></ion-icon>
								<ion-label class="ion-text-wrap">{{ p.title }}</ion-label>
							</ion-item>
						</ion-menu-toggle>

						<ion-item button *ngIf="p.children?.length > 0" (click)="p.open = !p.open"
							[class.active-parent]="p.open" detail="false" class="ParentMenu">
							<ion-icon name="{{p.icon}}" slot="start" class="menu-icon"></ion-icon>
							<ion-label class="ion-text-wrap">{{ p.title }}</ion-label>
							<ion-icon slot="end" name="caret-down-circle-outline" *ngIf="!p.open"></ion-icon>
							<ion-icon slot="end" name="caret-up-circle-outline" *ngIf="p.open"></ion-icon>
						</ion-item>

						<ion-list *ngIf="p.open">
							<ion-menu-toggle>
								<div *ngFor="let sub of p.children">
									<ion-item [routerLink]="[sub.url]" routerDirection="root" routerLinkActive="active"
										lines="none" detail="false" class="ChildMenu">
										<ion-icon name="{{sub.icon}}" slot="start" class="menu-icon"></ion-icon>
										<ion-label class="ion-text-wrap">{{ sub.title }}</ion-label>
									</ion-item>
								</div>
							</ion-menu-toggle>
						</ion-list>
					</div>
					<ion-menu-toggle auto-hide="false">
						<ion-item lines="none" (click)="logOut()" class="ParentMenu">
							<ion-icon slot="start" name="log-out-outline"></ion-icon>
							<ion-label> {{logoutLabel}} </ion-label>
						</ion-item>
					</ion-menu-toggle>
				</div>

				<!-- Institution menu -->
				<div *ngIf="IsIndividual == false && IsInstitution == true">
					<ion-menu-toggle auto-hide="false">
						<ion-item lines="none" (click)="goto_Dashboard()" class="ParentMenu">
							<ion-icon slot="start" name="home"></ion-icon>
							<ion-label> {{dashboardLabel}} </ion-label>
						</ion-item>
					</ion-menu-toggle>
					<div *ngFor="let p of AfterLoginInstitution">
						<ion-menu-toggle *ngIf="p.url">
							<ion-item [routerLink]="[p.url]" routerDirection="root" routerLinkActive="active"
								lines="none" detail="false" class="ParentMenu">
								<ion-icon name="{{p.icon}}" slot="start" class="menu-icon"></ion-icon>
								<ion-label class="ion-text-wrap">{{ p.title }}</ion-label>
							</ion-item>
						</ion-menu-toggle>

						<ion-item button *ngIf="p.children?.length > 0" (click)="p.open = !p.open"
							[class.active-parent]="p.open" detail="false" class="ParentMenu">
							<ion-icon name="{{p.icon}}" slot="start" class="menu-icon"></ion-icon>
							<ion-label class="ion-text-wrap">{{ p.title }}</ion-label>
							<ion-icon slot="end" name="caret-down-circle-outline" *ngIf="!p.open"></ion-icon>
							<ion-icon slot="end" name="caret-up-circle-outline" *ngIf="p.open"></ion-icon>
						</ion-item>

						<ion-list *ngIf="p.open">
							<ion-menu-toggle>
								<div *ngFor="let sub of p.children">
									<ion-item [routerLink]="[sub.url]" routerDirection="root" routerLinkActive="active"
										lines="none" detail="false" class="ChildMenu">
										<ion-icon name="{{sub.icon}}" slot="start" class="menu-icon"></ion-icon>
										<ion-label class="ion-text-wrap">{{ sub.title }}</ion-label>
									</ion-item>
								</div>
							</ion-menu-toggle>
						</ion-list>
					</div>
					<ion-menu-toggle auto-hide="false">
						<ion-item lines="none" (click)="logOut()" class="ParentMenu">
							<ion-icon slot="start" name="log-out-outline"></ion-icon>
							<ion-label> {{logoutLabel}} </ion-label>
						</ion-item>
					</ion-menu-toggle>
				</div>
			</ion-content>
			<div style="text-align: center; color: orange;background: #fff6f8;">
				<ion-label>{{versionLabel}}: 2.9</ion-label>
			</div>
		</ion-menu>
		<ion-router-outlet id="main-content"></ion-router-outlet>
	</ion-split-pane>
</ion-app>